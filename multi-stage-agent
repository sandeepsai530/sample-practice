pipeline { 
  agent none 
  stages {
    stage('Backe-end') {
      agent {
        docker { image 'maven:3.8.1-adotopen-jdk-11' }
      }
      steps {
        sh 'mvn --version'
        }
      }
    stage('Front-end') {
      agent { 
        docker { image 'node:16-alpine' }
      }
      steps {
      sh 'node --version'
      }
    }
  }
}
